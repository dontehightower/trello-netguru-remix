{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "extends": ["eslint:recommended", "prettier", "prettier/react"],
  "plugins": ["react/recommended", "react-hooks"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "valid-typeof": "error"
  }
}
